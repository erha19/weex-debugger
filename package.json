{
  "name": "weex-debugger",
  "description": "weex developer tool for debugging weex app with chrome devtool",
  "version": "1.0.24",
  "author": "erha19 <faterrole@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/weexteam/weex-debugger.git"
  },
  "keywords": [
    "weex",
    "debugger",
    "inspector",
    "devtool"
  ],
  "scripts": {
    "lint": "eslint src --fix",
    "dev": "babel src -w --out-dir lib",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run lint && npm run build && node ./scripts/update.js"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^8.0.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^2.13.1"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "boxen": "^1.2.2",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "del": "^2.2.1",
    "detect-port": "^1.2.1",
    "download": "^5.0.3",
    "exit": "^0.1.2",
    "fs-extra": "^7.0.0",
    "invok": "^1.0.2",
    "ip": "^1.1.3",
    "ipromise": "^1.0.2",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^5.4.0",
    "koa-static": "^5.0.0",
    "koa-websocket": "^2.1.0",
    "mkdirp": "^0.5.1",
    "node-simctl": "^3.11.1",
    "node-watch": "^0.3.5",
    "opn": "^4.0.2",
    "ora": "^2.0.0",
    "parse-hosts": "^1.0.1",
    "puppeteer": "^1.0.0",
    "request": "^2.83.0",
    "weex-builder": "^0.3.18",
    "ws": "^2.2.0"
  },
  "bin": {
    "weex-debugger": "bin/weex-devtool.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "changelog": {
    "messages": [
      "[Feature] Improve the debugging logic of weex in the sandbox debugging environment."
    ],
    "url": "https://github.com/weexteam/weex-debugger/blob/master/CHANGELOG.md"
  }
}
